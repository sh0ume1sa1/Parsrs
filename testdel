open(ALLFILE, "find ../book -type f -not -path '*/짜.*' |");
@allfile = <ALLFILE>;
close(IN);
open(RELEASEFILE,"< rls.lt");
@rlsfile =<RELEASEFILE>;
foreach $file (@allfile){
        print "$file------------짜n";
        foreach $rlsfile(@rlsfile){
                if ($file ne $rlsfile){
                        print ("del짜n");
                }else{
                        print ('!!!!!!!not del '.$rlsfile.'짜n');
                }

        }

}
